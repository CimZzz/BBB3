<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
          package="com.gogoh5.apps.quanmaomao.android">

    <uses-sdk tools:overrideLibrary="com.facebook.react"/>

    <application>
        <activity android:name=".ui.launch.LaunchUI"
            android:theme="@style/NoTitleBarStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    
        <activity android:name=".ui.main.MainUI"
                  android:launchMode="singleTask"
                  android:theme="@style/NoActionBarPortraitStyle">
        </activity>
    
        <activity android:name=".ui.search.SearchUI"
                  android:theme="@style/NoActionBarPortraitStyle">
        </activity>
    
        <activity android:name=".ui.productdetail.ProductDetailUI"
                  android:theme="@style/NoActionBarPortraitStyle">
        </activity>
    
        <activity android:name=".ui.web.WebUI"
                  android:theme="@style/NoActionBarPortraitStyle">
        </activity>
    
        <activity android:name=".ui.setting.SettingUI"
                  android:theme="@style/NoActionBarPortraitStyle">
        </activity>
    
        <activity android:name=".ui.cash.CashUI"
                  android:theme="@style/NoActionBarPortraitStyle">
        </activity>
    
        <activity android:name=".ui.balancedetail.BalanceDetailUI"
                  android:theme="@style/NoActionBarPortraitStyle">
        </activity>

        <activity android:name=".ui.aliauth.dialog.AliAuthDialogUI"
            android:theme="@style/DialogStyle"/>
        
        <activity android:name=".ui.aliauth.ui.AliAuthUI"
                  android:theme="@style/TransparentStyle"/>
    </application>

</manifest>